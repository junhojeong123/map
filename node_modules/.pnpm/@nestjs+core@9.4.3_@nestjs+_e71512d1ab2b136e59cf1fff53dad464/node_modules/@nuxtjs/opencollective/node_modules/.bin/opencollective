#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/opencollective/bin/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/opencollective/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules:/mnt/c/map/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/opencollective/bin/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/opencollective/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules/@nuxtjs/node_modules:/mnt/c/map/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2_encoding@0.1.13/node_modules:/mnt/c/map/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/opencollective.js" "$@"
else
  exec node  "$basedir/../../bin/opencollective.js" "$@"
fi
