#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/map/node_modules/.pnpm/typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules/typeorm/node_modules:/mnt/c/map/node_modules/.pnpm/typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules:/mnt/c/map/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/map/node_modules/.pnpm/typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules/typeorm/node_modules:/mnt/c/map/node_modules/.pnpm/typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules:/mnt/c/map/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.25_reflect-metadata@0.1.14_sqlite3@5.1.7/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
