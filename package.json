
{
  "name": "nestjs-app",
  "version": "1.0.0",
  "description": "A simple NestJS application",
  "main": "dist/main.js",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "nest build",
    "start": "pnpm run clean && pnpm run build && node dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10",
    "@nestjs/platform-express": "^10",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "jest": "^27.0.0",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}